{
    "name": "@twistly/shelf",
    "version": "0.1.0",
    "description": "A webhosting manager",
    "license": "MIT",
    "repository": "twistly/shelf",
    "author": {
        "name": "Alexis Tyler",
        "email": "xo@wvvw.me",
        "url": "github.com/twistly/shelf"
    },
    "publishConfig": {
